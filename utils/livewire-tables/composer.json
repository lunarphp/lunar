{
  "name": "lunarphp/livewire-tables",
  "type": "project",
  "description": "Livewire Tables",
  "keywords": ["lunar", "laravel", "livewire", "tables"],
  "license": "MIT",
  "authors": [
    {
      "name": "LunarPHP",
      "homepage": "https://lunarphp.io/"
    }
  ],
  "require": {
      "php": "^8.1",
      "livewire/livewire": "^2.0"
  },
  "require-dev": {
      "phpunit/phpunit": "^9.5",
      "mockery/mockery": "^1.4.4",
      "orchestra/testbench": "^7.0|^8.0",
      "laravel/pint": "^1.1",
      "brianium/paratest": "^6.10"
  },
  "autoload": {
      "psr-4": {
          "Lunar\\LivewireTables\\": "src/"
      }
  },
  "autoload-dev": {
      "psr-4": {
          "Lunar\\LivewireTables\\Tests\\": "tests/"
      }
  },
  "extra": {
    "laravel": {
        "providers": [
            "Lunar\\LivewireTables\\LivewireTablesServiceProvider"
        ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
