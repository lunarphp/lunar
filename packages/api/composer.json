{
    "name": "lunarphp/api",
    "description": "Lunar API, Laravel e-commerce core functionality.",
    "keywords": ["lunar", "laravel", "ecommerce", "e-commerce", "headless", "store", "shop", "cart"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
          "name": "Lunar",
          "homepage": "https://lunarphp.io/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Lunar\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lunar\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lunar\\ApiServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.1",
        "lunarphp/lunar": "dev-feature/laravel_10",
        "laravel-json-api/laravel": "^2.6|^3.0",
        "laravel-json-api/hashids": "2.x-dev"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0",
        "phpunit/phpunit": "^9.5",
        "laravel-json-api/testing": "^2.1"
    },
    "minimum-stability" : "dev"
}
